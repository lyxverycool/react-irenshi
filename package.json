{
  "name": "react-mobile-irenshi",
  "version": "1.4.1",
  "description": "A boilerplate for React-Babel-Webpack project",
  "main": "app/main.jsx",
  "scripts": {
    "precommit": "npm run lint",
    "commitmsg": "npm run lint:msg",
    "lint": "eslint --cache --ignore-path .gitignore --ext .jsx,.js --format=node_modules/eslint-formatter-pretty .",
    "lint:msg": "validate-commit-msg",
    "test": "npm run lint",
    "build": "webpack",
    "start": "webpack-dev-server --devtool eval --progress --hot --host 0.0.0.0 --content-base app",
    "deploy": "NODE_ENV=production webpack --config webpack.production.config.js",
    "deploy-windows": "SET NODE_ENV=production & webpack --config webpack.production.config.js",
    "validate": "npm ls",
    "commit": "git cz",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 1"
  },
  "dependencies": {
    "babel-runtime": "6.x",
    "es6-promise": "^4.0.5",
    "isomorphic-fetch": "^2.2.1",
    "react": "15.x",
    "react-dom": "15.x",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "redux": "^3.6.0",
    "redux-immutablejs": "^0.0.8",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.1.0",
    "whatwg-fetch": "^2.0.2"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "font-awesome": "^4.7.0",
    "bootstrap": "3.3.x",
    "babel-core": "6.x",
    "babel-eslint": "7.x",
    "babel-loader": "6.x",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-runtime": "6.x",
    "babel-preset-es2015": "6.x",
    "babel-preset-react": "6.x",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "6.x",
    "commitizen": "^2.9.3",
    "conventional-changelog-cli": "1.x",
    "copy-webpack-plugin": "latest",
    "css-loader": "~0.26.1",
    "cz-conventional-changelog": "^1.2.0",
    "eslint": "latest",
    "eslint-config-airbnb": "latest",
    "eslint-formatter-pretty": "^1.1.0",
    "eslint-plugin-compat": "^1.0.0",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jsx-a11y": "3.x",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-react": "latest",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.22.0",
    "html-withimg-loader": "^0.1.16",
    "http-proxy-middleware": "^0.17.3",
    "husky": "^0.12.0",
    "node-sass": "^4.5.0",
    "open-browser-webpack-plugin": "0.0.3",
    "sass": "^0.5.0",
    "sass-loader": "^4.0.2",
    "style-loader": "~0.13.1",
    "url-loader": "^0.5.6",
    "svg-url-loader":"",
    "validate-commit-msg": "2.x",
    "webpack": "1.x",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-dev-server": "1.x",
    "webpack-hot-middleware": "^2.13.2"
  },
  "keywords": [
    "es6",
    "web app",
    "boilerplate"
  ],
  "author": "cool",
  "license": "MIT",
  "config": {
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert"
      ],
      "warnOnFail": false,
      "maxSubjectLength": 100
    },
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
